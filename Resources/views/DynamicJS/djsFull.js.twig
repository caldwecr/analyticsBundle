$(function () {
    var cObj = {
        s: function (o, eventType, selectorId) {
            var callbackUrl = '{{ callbackUrl|raw }}';
            callbackUrl = callbackUrl.replace('eventKeyStub', eventType);
            callbackUrl = callbackUrl.replace('selectorKeyStub', selectorId);
            var p = {
                clientX: o.clientX,
                clientY: o.clientY,
                classList: o.currentTarget.classList,
                id: o.currentTarget.id,
                outerHTML: o.currentTarget.outerHTML,
                eventType: o.type
            };
            $.ajax({
                url: callbackUrl,
                type: 'POST',
                dataType: 'json',
                data: JSON.stringify(p)
            });
        }
    };
{% for event in events %}
    {% for selector in selectors %}
        $('{{ selector.selection|raw }}').{{ event.eventName }}(function (o) {
            cObj.s(o, o.type, '{{ selector.id}}');
        });
    {% endfor %}
{% endfor %}
});